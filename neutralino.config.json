{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vue-src/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/vue-src/public/favicon.ico"
    }
  },
  "cli": {
    "binaryName": "myapp",
    "resourcesPath": "/vue-src/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/vue-src/public/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0"
  }
}